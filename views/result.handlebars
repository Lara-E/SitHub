<!--  Sitter Results -->
<nav>
  <div class="nav-wrapper">
    <a href="#" class="brand-logo">Sithub</a>
    <ul id="nav-mobile" class="right hide-on-med-and-down">
      <li><a href="/">Home</a></li>
      <li>|</li>
      <li><a href="/sitter">Sign Up</a></li>
      <li>|</li>
      <li><a href="/login">Sign In</a></li>
    </ul>
  </div>
</nav>
<div class="container">
    <div class="row">
        {{!-- <div class="col s3">

        </div> --}}
        <div id="content" class="col s6 z-depth-3">
            <content>
                <h3 class="center">Sitters</h3>
                <div class="container">
                    <div class="row">
                        
                    </div>
                </div>


                <ul class="collection font">
                    {{#each sitter}}
                    <img src="{{pic}}" alt="sitter" width="300" height="auto">
                    <br>
                    {{firstName}} {{lastName}}
                    <br>
                    {{#if knowsCPR}}
                    <i class="far fa-check-square checks"> <span class="checks font">Knows CPR</span></i>
                    {{/if}}
                    {{#if petsOk}}
                    <i class="far fa-check-square checks"> <span class="checks font">Pets OK</span></i>
                    {{/if}}
                    {{#if hasReferences}}
                    <i class="far fa-check-square checks"> <span class="checks font">Has References</span></i>
                    {{/if}}
                    {{#if hasTransportation}}
                    <i class="far fa-check-square checks"> <span class="checks font">Has Transportation</span></i>
                    {{/if}}
                    {{#if comments}}
                    <p class="font">Comments and/or Questions: {{comments}}</p>
                    {{/if}}
                    <p>{{email}}</p>
                    <hr>
                    {{/each}}
                    
                </ul>
                
            </content>
        </div>
        <div class="col s3">

        </div>

    </div>
</div>