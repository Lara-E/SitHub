<!--  Sitter Results -->

<div class="container">
    <div class="row">
        <div class="col s3">

        </div>
        <div id="content" class="col s6">
            <content>
                <h3>Sitters</h3>
                <ul class="collection">
                    {{#each sitter}}
                    <img src="{{pic}}" alt="sitter" width="150px" height="auto">
                    {{firstName}} {{lastName}}
                    {{#if knowsCPR}}
                    <i class="far fa-check-square"></i>
                    {{/if}}

                    {{/each}}
                </ul>
            </content>
        </div>
        <div class="col s3">

        </div>

    </div>
</div>